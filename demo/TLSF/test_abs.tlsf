	(STATE[4] && ACTION[1]) -> ( X STATE[4] );
	(STATE[0] && ACTION[2]) -> ( X STATE[4] );
	(STATE[1] && ACTION[2]) -> ( X STATE[0] );
	(STATE[2] && ACTION[2]) -> ( X STATE[4] );
