	INPUTS {
	STATE[n];
	}
	OUTPUTS {
	ACTION[m];
	}
	INITIALLY{
	!(STATE[2] || STATE[4]);
	mutual(STATE);
	}
	PRESET {
	}
	REQUIRE {
	mutual(STATE);
	(STATE[0] && ACTION[0]) -> ( X STATE[0] );
	(STATE[1] && ACTION[0]) -> ( X STATE[4] );
	(STATE[2] && ACTION[0]) -> ( X STATE[4] );
