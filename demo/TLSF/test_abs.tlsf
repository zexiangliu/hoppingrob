INFO{
	TITLE: "Controller synthesis for continuousdynamical systems"
	DESCRIPTION:"none"
	SEMANTICS:   Mealy,Strict
	TARGET:   Mealy
}
GLOBAL  {
	PARAMETERS {
		n = 5; //num of states
		m = 9; //num of actions
	}
	DEFINITIONS {
	mutual(b) =
	||[i IN {0, 1 .. (SIZEOF b) -1}]
	&&[j IN {0,1 .. (SIZEOF b) -1} (\) {i}]
	(b[i] && !b[j]);
	}
}
